{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
   "crons": [
    {
      "path": "/job/subscriptionExpiry",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/job/oldMeetings",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/job/scheduledNotifications",
       "schedule": "*/1 * * * *"
    },
    {
      "path": "/job/deleteOldNotifications",
      "schedule": "0 0 * * *"
    }
  ]
}
